@model Assignment61ViewModel

@if (Model.AccessLevel < 2)
{
    <p>You do not have a sufficient access level to view this data.</p>
}
else if (Model.AccessLevel >= 2 && Model.AccessLevel < 8)
{
    <ul>
        @foreach (var student in Model.Students)
        {
            <li>@student.FirstName @student.LastName</li>
        }
    </ul>
}
else
{
    <h2>Hello Admin</h2>
    <ul>
        @foreach (var student in Model.Students)
        {
            <li>@student.FirstName @student.LastName (@student.Grade)</li>
        }
    </ul>
}
